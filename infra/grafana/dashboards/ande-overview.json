{
  "dashboard": {
    "title": "ANDE Chain - System Overview",
    "tags": ["ande", "overview"],
    "timezone": "browser",
    "schemaVersion": 36,
    "version": 1,
    "refresh": "10s",
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "panels": [
      {
        "id": 1,
        "title": "Block Production Rate",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0},
        "targets": [
          {
            "expr": "rate(ande_blocks_proposed_total[5m])",
            "legendFormat": "Blocks/sec",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "short", "label": "Blocks/sec"},
          {"format": "short"}
        ]
      },
      {
        "id": 2,
        "title": "Transaction Throughput (TPS)",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0},
        "targets": [
          {
            "expr": "ande_parallel_throughput_tps",
            "legendFormat": "Current TPS",
            "refId": "A"
          },
          {
            "expr": "rate(ande_parallel_execution_success_total[1m]) * 100",
            "legendFormat": "Avg TPS (1m)",
            "refId": "B"
          }
        ],
        "yaxes": [
          {"format": "short", "label": "TPS"},
          {"format": "short"}
        ]
      },
      {
        "id": 3,
        "title": "Parallel Execution Performance",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8},
        "targets": [
          {
            "expr": "rate(ande_parallel_execution_success_total[5m])",
            "legendFormat": "Successful Executions/sec",
            "refId": "A"
          },
          {
            "expr": "rate(ande_parallel_execution_conflicts_total[5m])",
            "legendFormat": "Conflicts/sec",
            "refId": "B"
          }
        ],
        "yaxes": [
          {"format": "short", "label": "Operations/sec"},
          {"format": "short"}
        ],
        "alert": {
          "conditions": [
            {
              "evaluator": {"params": [0.3], "type": "gt"},
              "query": {"params": ["B", "5m", "now"]},
              "reducer": {"params": [], "type": "avg"},
              "type": "query"
            }
          ],
          "name": "High Conflict Rate"
        }
      },
      {
        "id": 4,
        "title": "Conflict Rate",
        "type": "gauge",
        "gridPos": {"h": 8, "w": 6, "x": 12, "y": 8},
        "targets": [
          {
            "expr": "100 * (rate(ande_parallel_execution_conflicts_total[5m]) / rate(ande_parallel_execution_success_total[5m]))",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "green"},
                {"value": 20, "color": "yellow"},
                {"value": 30, "color": "red"}
              ]
            }
          }
        }
      },
      {
        "id": 5,
        "title": "Active Workers",
        "type": "stat",
        "gridPos": {"h": 8, "w": 6, "x": 18, "y": 8},
        "targets": [
          {
            "expr": "ande_parallel_workers_active",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "red"},
                {"value": 8, "color": "yellow"},
                {"value": 16, "color": "green"}
              ]
            }
          }
        }
      },
      {
        "id": 6,
        "title": "MEV Value Extracted",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 16},
        "targets": [
          {
            "expr": "rate(ande_mev_value_extracted_wei[1h]) / 1e18",
            "legendFormat": "ETH Extracted per Hour",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "currencyUSD", "label": "Value (ETH)"},
          {"format": "short"}
        ]
      },
      {
        "id": 7,
        "title": "MEV Distribution",
        "type": "piechart",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 16},
        "targets": [
          {
            "expr": "ande_mev_validator_share_wei",
            "legendFormat": "Validators (80%)",
            "refId": "A"
          },
          {
            "expr": "ande_mev_protocol_share_wei",
            "legendFormat": "Protocol (20%)",
            "refId": "B"
          }
        ]
      },
      {
        "id": 8,
        "title": "Data Availability Submissions",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 24},
        "targets": [
          {
            "expr": "rate(ande_da_submissions_total[5m])",
            "legendFormat": "Submissions/sec",
            "refId": "A"
          },
          {
            "expr": "rate(ande_da_verifications_success_total[5m])",
            "legendFormat": "Successful Verifications/sec",
            "refId": "B"
          },
          {
            "expr": "rate(ande_da_verifications_failed_total[5m])",
            "legendFormat": "Failed Verifications/sec",
            "refId": "C"
          }
        ]
      },
      {
        "id": 9,
        "title": "DA Submission Latency (P95)",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 24},
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(ande_da_submission_latency_seconds_bucket[5m]))",
            "legendFormat": "P95 Latency",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.99, rate(ande_da_submission_latency_seconds_bucket[5m]))",
            "legendFormat": "P99 Latency",
            "refId": "B"
          }
        ],
        "yaxes": [
          {"format": "s", "label": "Latency"},
          {"format": "short"}
        ]
      },
      {
        "id": 10,
        "title": "Validator Participation",
        "type": "gauge",
        "gridPos": {"h": 8, "w": 8, "x": 0, "y": 32},
        "targets": [
          {
            "expr": "ande_validator_participation_rate_percent",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "min": 0,
            "max": 100,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "red"},
                {"value": 50, "color": "yellow"},
                {"value": 67, "color": "green"}
              ]
            }
          }
        }
      },
      {
        "id": 11,
        "title": "Time to Finality",
        "type": "graph",
        "gridPos": {"h": 8, "w": 8, "x": 8, "y": 32},
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(ande_finality_time_seconds_bucket[5m]))",
            "legendFormat": "P95",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.50, rate(ande_finality_time_seconds_bucket[5m]))",
            "legendFormat": "P50 (Median)",
            "refId": "B"
          }
        ],
        "yaxes": [
          {"format": "s", "label": "Time"},
          {"format": "short"}
        ]
      },
      {
        "id": 12,
        "title": "Network Peers",
        "type": "stat",
        "gridPos": {"h": 8, "w": 8, "x": 16, "y": 32},
        "targets": [
          {
            "expr": "ande_peer_count",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "red"},
                {"value": 5, "color": "yellow"},
                {"value": 10, "color": "green"}
              ]
            }
          }
        }
      },
      {
        "id": 13,
        "title": "RPC Request Rate by Method",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 40},
        "targets": [
          {
            "expr": "rate(ande_rpc_requests_total[5m])",
            "legendFormat": "{{ method }}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "reqps", "label": "Requests/sec"},
          {"format": "short"}
        ]
      },
      {
        "id": 14,
        "title": "RPC Latency Distribution",
        "type": "heatmap",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 40},
        "targets": [
          {
            "expr": "rate(ande_rpc_request_duration_seconds_bucket[5m])",
            "format": "heatmap",
            "refId": "A"
          }
        ],
        "dataFormat": "tsbuckets"
      }
    ]
  }
}
